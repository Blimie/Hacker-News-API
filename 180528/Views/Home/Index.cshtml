@using _180528.Api
@model IEnumerable<Story>

<table class="table table-bordered table-striped table-hover" style="margin-top: 60px">
    <tr>
        <th>Title (Click to view story)</th>
        <th>Score</th>
    </tr>
    @foreach (Story s in Model)
    {
        <tr>
            <td> <a href="@s.Url">@s.Title</a> </td>
            <td>@s.Score</td>
        </tr>
    }
</table>
